SELECT min(month), count(driver_id) FROM (SELECT min(dttm) as month, driver_id, count(order_id) as amount_of_orders FROM fediq_team.order_tables_201801 WHERE 64<=start_point_a_lat and start_point_a_lat<=65 GROUP BY driver_id) as A WHERE amount_of_orders > 1
UNION
