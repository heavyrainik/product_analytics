SELECT min(dttm), car_class, round(sum(order_cost)/count(order_id)) as mean_check, count(order_id) FROM fediq_team.order_tables_201801 WHERE status = 'complete' and dttm IS NOT NULL and 64<=start_point_a_lat and start_point_a_lat<=65 GROUP BY car_class
